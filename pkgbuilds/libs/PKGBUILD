pkgver=1.0.0
pkgrel=1
arch=('x86_64')
pkgdesc="Dependencies for compilation of programs"
depends=(
    'libapr1-dev'   # Needed by some of the Ada benchmarks
    'libhts-dev'    # Needed by C benchmarks
)

build() {
    return 0
}

package() {
    return 0
}